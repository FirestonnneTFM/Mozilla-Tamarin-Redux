package p{
   public class MyCls
   {
        public static var AddTestCase:Function;
        public static function foo(x:*):*
        {
           AddTestCase( "typeof ",        "float",        typeof(x) );
           AddTestCase( "is Number ", false, x is Number);
           AddTestCase( "is float ",  true , x is float);
           return x;
        }

        public static function foo2(x:*):*{
          return ++x;
        }
   }
}
